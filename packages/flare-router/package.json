{
  "name": "flare-router",
  "version": "1.0.0",
  "description": "Blazingly fast SPA-like router for static sites - Pure vanilla JS",
  "main": "dist/main.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/dunamismax/flare-router.git"
  },
  "homepage": "https://github.com/dunamismax/flare-router#readme",
  "scripts": {
    "build": "pnpm run build:main",
    "build:main": "esbuild lib/main.js --bundle --outfile=dist/main.js --format=esm",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "router",
    "spa",
    "navigation",
    "vanilla-js",
    "prefetch",
    "static-sites",
    "flare",
    "lightweight"
  ],
  "author": "dunamismax <dunamismax@tutamail.com>",
  "license": "MIT",
  "devDependencies": {
    "esbuild": "^0.23.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.15.0",
    "prettier": "^2.7.1"
  }
}
